
{
  "manifest_version": 3,
  "name": "Chatgpt3.6 Extension",
  "version": "1.0.0",
  "description": "Integrates Chatgpt with Google search.",
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "action": {
    "default_title": "AI's edu"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
    
  },
  "permissions": ["storage","activeTab", "clipboardWrite","sidePanel", "tabs"],
  "options_page": "option.html",
  

  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  },
  "icons": {
    	   "16": "./image/robot.png",
            "32": "./image/robot.png",
            "48": "./image/robot.png",
            "128": "./image/robot.png"
  },
  "content_scripts": [
	{
      "matches": [
        "*://*/*",
         "<all_urls>"
      ],
      "js": ["content.js","Summary.js","wordcloudmaking.js","service-worker.js"]
		}
	]
}
