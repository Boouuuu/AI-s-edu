
{
  "manifest_version": 3,
  "name": "Chatgpt3.6 Extension",
  "version": "1.0.0",
  "description": "Integrates Chatgpt with Google search.",
  "content_scripts": [
    {
        "matches": [
          "*://*/*",
           "<all_urls>"
        ],
        "js": [ "libs/jquery-3.7.1.min.js", "libs/jquery-ui.min.js", "libs/jquery-1.7.1.min.js",
        "libs/jquery.min.js","content.js","summary.js","aftertest.js","Real-time_monitoring.js","service-worker.js","Error_summary.js"]
      }
    ],

  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_title": "AI's edu"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "permissions": ["storage","activeTab", "clipboardWrite","sidePanel", "tabs"],
  "options_page": "option.html",
  

  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  },
  "icons": {
    	   "16": "./image/robot.png",
            "32": "./image/robot.png",
            "48": "./image/robot.png",
            "128": "./image/robot.png"
  }

}
